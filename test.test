package main

import (
	"github.com/jmoiron/sqlx"

	_ "github.com/mattn/go-sqlite3"
)

func main() {
	DB, err := sqlx.Open("sqlite3", "adb.db")
	if err != nil {
		panic(err)
	}

	type Name struct {
		Name string `db:"Name"`
	}
	names := []Name{}
	err = DB.Select(&names, "SELECT name FROM activists")
	if err != nil {
		panic(err)
	}

	ret := []string{}
	for _, n := range names {
		ret = append(ret, n.Name)
	}

}
